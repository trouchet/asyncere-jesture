{
  "name": "mvp-jest",
  "version": "1.0.0",
  "description": "An example application for demonstrating the usage of supertest for writing unit tests for node api's",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --trace-warnings ./src/index.js",
    "test": "jest --collect-coverage --coverage --detectOpenHandles",
    "test:watch": "jest --watchAll --collect-coverage --coverage --detectOpenHandles"
  },
  "author": "Bruno Peixoto",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.17.1",
    "nodemon": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "jest": "^29.3.1",
    "supertest": "^6.0.0"
  }
}
